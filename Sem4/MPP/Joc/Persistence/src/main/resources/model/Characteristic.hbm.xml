<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="app.model.Characteristic" table="characteristics">
        <id name="characteristic" column="characteristic" type="string"/>

        <many-to-one name="word" class="app.model.Word"/>
    </class>
</hibernate-mapping>
